{
  "name": "rivine-wallet",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build:prod": "rm -rf www && ionic-app-scripts build --prod --nobrowser --enableLint false && node tools/finish-production-build.js",
    "build:zip": "node tools/set-config.js",
    "postbuild:zip": "npm run build:prod && npm run build:zip-only",
    "build:zip-only": "rm -f wallet.zip && mv www/index.html www/cordova.html && cd www && zip -r ../wallet.zip . && cd .. && mv www/cordova.html www/index.html",
    "build:embedded-app": "npm run build:prod && rm -f wallet.zip && cd www && zip -r ../wallet.zip .",
    "lint": "ionic-app-scripts lint",
    "start": "node tools/set-config.js",
    "poststart": "ionic-app-scripts serve",
    "sort_translations": "node tools/sort-i18n.js"
  },
  "dependencies": {
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@ionic-native/browser-tab": "^5.0.0-beta.21",
    "@ionic-native/core": "^5.0.0-beta.21",
    "@ionic-native/in-app-browser": "^5.0.0-beta.21",
    "@ionic-native/splash-screen": "^5.0.0-beta.21",
    "@ionic-native/status-bar": "^5.0.0-beta.21",
    "@ionic/storage": "^2.2.0",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular2-qrcode": "^2.0.1",
    "cordova-plugin-console": "^1.1.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-inappbrowser": "^3.0.0",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "ionic-angular": "^3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "4.4.4",
    "minimist": "^1.2.0",
    "ngx-clipboard": "^11.1.7",
    "rogerthat-plugin": "git+https://github.com/our-city-app/cordova-mobicage-plugin.git#master",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "sw-toolbox": "3.6.0",
    "zone.js": "^0.8.26"
